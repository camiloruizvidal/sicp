#ErrorDocument 404 errornotfound
Options +FollowSymLinks
RewriteEngine on
Options -Indexes
Rewriterule ^ajax(.*)/(.*)$ controller/anico_ajax.php?control=$1&function=$2
Rewriterule ^sicp/(.*)_(.*)$ view/form/$1/$2.php
Rewriterule ^files/(.*)$ view/files/$1
Rewriterule ^css/(.*)$ view/css/$1
Rewriterule ^img/(.*)$ view/img/$1
Rewriterule ^js/(.*)$ view/js/$1

